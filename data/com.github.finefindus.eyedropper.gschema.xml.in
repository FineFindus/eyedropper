<?xml version="1.0" encoding="utf-8"?>
<schemalist>
  <schema path="/com/github/finefindus/eyedropper/" id="@app-id@" gettext-domain="@gettext-package@">
    <key name="window-width" type="i">
      <default>300</default>
      <summary>Window width</summary>
    </key>
    <key name="window-height" type="i">
      <default>350</default>
      <summary>Window height</summary>
    </key>
    <key name="is-maximized" type="b">
      <default>false</default>
      <summary>Window maximized state</summary>
    </key>
    <key name="format-order" type="as">
      <default>[ 'hex', 'rgb', 'hsl', 'hsv', 'cmyk', 'xyz', 'cielab', 'hwb', 'hcl' ]</default>
      <summary>Order of the formats</summary>
      <description>In which order the formats should be displayed. Only possible values should the format names.</description>
    </key>
    <key name="alpha-position" type="i">
      <default>0</default>
      <summary>Alpha Value Position</summary>
      <description>Where the alpha value is postioned in the hex string.</description>
    </key>
    <key name="show-hex-model" type="b">
      <default>true</default>
      <summary>Show Hex Model</summary>
      <description>Whether the application should show the hex code.</description>
    </key>
    <key name="show-rgb-model" type="b">
      <default>true</default>
      <summary>Show RGB Model</summary>
      <description>Whether the application should show the RGB code.</description>
    </key>
    <key name="show-hsl-model" type="b">
      <default>true</default>
      <summary>Show HSL Model</summary>
      <description>Whether the application should show the HSl code.</description>
    </key>
    <key name="show-hsv-model" type="b">
      <default>true</default>
      <summary>Show HSV Model</summary>
      <description>Whether the application should show the HSV code.</description>
    </key>
    <key name="show-cmyk-model" type="b">
      <default>true</default>
      <summary>Show CMYK Model</summary>
      <description>Whether the application should show the CMYK code.</description>
    </key>
    <key name="show-xyz-model" type="b">
      <default>false</default>
      <summary>Show XYZ Model</summary>
      <description>Whether the application should show the XYZ code.</description>
    </key>
    <key name="show-cie-lab-model" type="b">
      <default>false</default>
      <summary>Show CIELAB Model</summary>
      <description>Whether the application should show the CIELAB code.</description>
    </key>
    <key name="show-hwb-model" type="b">
      <default>false</default>
      <summary>Show HWB Model</summary>
      <description>Whether the application should show the HWB code.</description>
    </key>
    <key name="show-hcl-model" type="b">
      <default>false</default>
      <summary>Show HCL Model</summary>
      <description>Whether the application should show the HCL code.</description>
    </key>
  </schema>
</schemalist>